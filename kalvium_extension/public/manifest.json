{
    "manifest_version": 3,
    "name": "KalviumLearnTube",
    "version": "1.0.0", 
    "key": "",
    "description": "Provide free education for everyone, supporting both students who excel and those who struggle, ensuring no one is left behind.",
    "permissions": ["scripting", "storage", "tabs", "activeTab", "contextMenus","identity","identity.email"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "oauth2": {
      "client_id": "810226153127-pqtquduqgf8vnfi5ljj674kodaon641c.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/youtube.readonly"
      ]
    },
    "commands": {
      "open-extension": {
        "suggested_key": {
          "default": "Ctrl+Shift+O",
          "mac": "Command+Shift+O"
        },
        "description": "Open the extension popup"
      },
      "extract-data": {
        "suggested_key": {
          "default": "Ctrl+Shift+X",
          "mac": "Command+Shift+X"
        },
        "description": "Extract selected text"
      },
      "save-event": {
        "suggested_key": {
          "default": "Ctrl+Shift+S",
          "mac": "Command+Shift+S"
        },
        "description": "Save the event"
      }
    }
  }
  